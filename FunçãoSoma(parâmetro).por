programa
{
	
	funcao inicio()
	{
		escreva("Olá Mundo \n")
		soma(20,30)
	}

	//função com parâmetro ()
	funcao soma(inteiro n1, inteiro n2){
		inteiro resultado = n1+n2
		escreva(resultado)
	}
}
